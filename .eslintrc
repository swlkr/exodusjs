{
  "env": {
    "browser": false,
    "es6":true,
    "node": true
  },
  "rules": {
    "new-cap": [0],
    "strict": [2, "never"],
    "quotes": [1, "single"],
    "no-process-exit": 0,
    "camelcase": 1,
    "comma-dangle": [0],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-multi-spaces": 0
  }
}
